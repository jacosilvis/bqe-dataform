CREATE OR REPLACE PROCEDURE ${self()}(seconds INT64)
BEGIN
    DECLARE DELAY_UNTIL DATETIME DEFAULT DATETIME_ADD(CURRENT_DATETIME, INTERVAL seconds SECOND);

    WHILE CURRENT_DATETIME <= DELAY_UNTIL DO
    END WHILE;
END